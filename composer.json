    "license": "MIT",
    "authors": [
        {
            "name": "Adam",
            "email": "jamiuadam120@gmail.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/adebayo/seamless",
    "keywords": ["Laravel", "Seamless"],
    "require": {
        "illuminate/support": "v8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Adebayo\\Seamless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adebayo\\Seamless\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adebayo\\Seamless\\SeamlessServiceProvider"
            ],
            "aliases": {
                "Seamless": "Adebayo\\Seamless\\Facades\\Seamless"
            }
        }
    }
}
